    "mocha": "^5.2.0",
    "react-dev-utils": "^6.1.1",
    "react-slider": "^0.9.0",
    "url": "^0.11.0",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
