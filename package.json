  "version": "0.2.1",
  "description": "Web-based chordal musical instrument",
  "main": "index.js",
  "scripts": {
    "dev": "NODE_ENV=development ./node_modules/.bin/webpack-dev-server --config ./config/webpack.config.js",
    "prod": "NODE_ENV=production ./node_modules/.bin/webpack --config ./config/webpack.config.prod.js",
    "test": "mocha --opts ./config/mocha.opts",
